<h1>Templates</h1>

<table>
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Date Created</th>
    <th>Actions</th>
  </thead> 
  <tbody>
    <% @templates.each do |template| %>
      <tr>
        <td><%= template.id.present? ? template.id : -1 %></td> 
        <td><%= template.template['name'].present? ? template.template['name'] : 'No name' %></td>
        <td><%= template.created_at.strftime('%m-%d-%Y') %></td>
        <td></td>
    <% end %>
  </tbody>
</table>